{
  "name": "grafeno.pagamentos/grafeno-sdk-php",
  "description": "",
  "keywords": ["Grafeno", "charges", "FIDC"],
  "license": "MIT",
  "type": "libraries",
  "support": {
     "issues": "https://github.com/mariodias/sdk-project-gra/issues",
     "source": "https://github.com/mariodias/sdk-project-gra/"
 },
  "authors": [
    {
      "name": "Mário Sérgio Araújo Dias",
      "email": "mario.dias@ufpi.edu.br"
    }
  ],
  "require": {
    "php": ">=6.0.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.5",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/var-dumper": "^3.3",
    "pestphp/pest": "1.x-dev"
  },
  "scripts": {
    "test": "./vendor/bin/pest"
  },
     "autoload": {
        "psr-4": {
            "Grafeno\\": "src/"
        }
      },
    "autoload-dev": {
        "psr-4": {
            "Grafeno\\Tests\\": "tests/"
        }
    },
    "extra": {
      "branch-alias": {
          "dev-master": "1.x-dev"
      }
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
